{
  "timeZone": "UTC",
  "dependencies": {
    "libraries": [
      {
        "userSymbol": "OAuth2",
        "version": "43",
        "libraryId": "1B7FSrk5Zi6L1rSxxTDgDEUsPzlukDsi4KGuTMorsTQHhGBzBkMun4iDF"
      }
    ]
  },
  "exceptionLogging": "STACKDRIVER",
  "runtimeVersion": "V8",
  "oauthScopes": [
    "https://www.googleapis.com/auth/documents",
    "https://www.googleapis.com/auth/script.external_request",
    "https://www.googleapis.com/auth/workspace.linkpreview"
  ],
  "urlFetchWhitelist": [
    "https://api.github.com/",
    "https://github.com/login/oauth/access_token"
  ],
  "addOns": {
    "common": {
      "name": "GitHub Smart Chips",
      "logoUrl": "https://ianlewis.github.io/github-smart-chips/logo.png",
      "layoutProperties": {
        "primaryColor": "#24292e",
        "secondaryColor": "#586069"
      },
      "homepageTrigger": {
        "runFunction": "onLinkPreview"
      },
      "universalActions": [],
      "openLinkUrlPrefixes": ["https://github.com/"]
    },
    "docs": {
      "linkPreviewTriggers": [
        {
          "runFunction": "onLinkPreview",
          "patterns": [
            {
              "hostPattern": "github.com"
            }
          ]
        }
      ]
    },
    "sheets": {
      "linkPreviewTriggers": [
        {
          "runFunction": "onLinkPreview",
          "patterns": [
            {
              "hostPattern": "github.com"
            }
          ]
        }
      ]
    },
    "slides": {
      "linkPreviewTriggers": [
        {
          "runFunction": "onLinkPreview",
          "patterns": [
            {
              "hostPattern": "github.com"
            }
          ]
        }
      ]
    }
  }
}
